package tictactoe;

/**
 * 
 * Enumerator for Xs and Os
 *
 */
public enum Symbol {
    X('X'), O('O'), EMPTY(' ');

    private char ch;

    void Marker(char ch) {
        this.ch = ch;
    }
    
    public char toChar() {
        return this.ch;
    }
    
    @Override
    public String toString() {
        return String.format("%c", this.ch);
    }
}
